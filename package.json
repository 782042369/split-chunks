    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "keywords": [
    "vite",
    "rolldown",
    "split-chunks",
    "plugin"
  ],
  "author": "waibuzheng",
  "license": "ISC",
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.0.2",
    "lint-staged": "^16.2.7",
    "rolldown-vite": "^7.2.7",
    "standard-version": "^9.5.0",
    "tsdown": "^0.16.7",
    "vite": "npm:rolldown-vite@^7.2.7"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@latest"
    }
  },
